<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane minHeight="311.0" prefHeight="311.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sisapsoo.controller.CadastroClienteController">
   <buttonTypes>
      <ButtonType fx:constant="OK" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
   <header>
      <AnchorPane minHeight="250.0" prefHeight="259.0" prefWidth="647.0" style="-fx-background-color: white;">
         <children>
            <Label fx:id="titulo" alignment="CENTER" contentDisplay="CENTER" layoutX="98.0" layoutY="14.0" text="Cadastro de Cliente" textAlignment="CENTER" textFill="#515151">
               <font>
                  <Font size="34.0" />
               </font>
               <padding>
                  <Insets bottom="15.0" top="15.0" />
               </padding>
            </Label>
            <TextField fx:id="campoNome" layoutX="65.0" layoutY="94.0" prefHeight="39.0" prefWidth="360.0" promptText="Nome" style="-fx-background-color: transparent; -fx-border-width: 0 0 1 0; -fx-border-color: b0b0b0; -fx-padding: 5 0 5 0;">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="campoTelefone" layoutX="65.0" layoutY="161.0" prefHeight="39.0" prefWidth="360.0" promptText="Telefone" style="-fx-background-color: transparent; -fx-border-width: 0 0 1 0; -fx-border-color: b0b0b0; -fx-padding: 5 0 5 0;">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Label fx:id="labelSalvo" layoutX="407.0" layoutY="532.0" text="Salvo!!" textAlignment="CENTER" visible="false">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </header>
</DialogPane>
